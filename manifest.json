{
    "background": {
        "scripts": [
            "background_script.js"
        ]
    },
    "manifest_version": 2,
    "name": "Open Salesforce developer console",
    "description": "Opens the Salesforce developer console with the Org you are currently logged in with",
    "version": "3.1",
    "icons": {
        "48": "icons/img.png"
    },
    "browser_action": {
        "default_icon": "icons/img.png"
    },
    "permissions": [
        "tabs"
    ],
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+Y",
                "windows": "Ctrl+Shift+Y",
                "mac": "Command+Shift+Y",
                "chromeos": "Ctrl+Shift+Y",
                "linux": "Ctrl+Shift+Y"
            }
        }
    }
}